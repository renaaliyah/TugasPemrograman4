public class MainAnimal {
     public static void main(String[] args) {
       Animal a = new Animal();
       Spider s = new Spider();
       Cat c = new Cat();
       Fish f = new Fish();
       
       a.walk();
       a.eat();    
       
       s.Spider();
       s.eat();
       
       c.play();
       c.eat();
       
       f.play();
       f.walk();
       f.eat();
       
       
       
    }
    
}
